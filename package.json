{
  "name": "@vcmap/walk",
  "version": "2.0.0-rc.0",
  "description": "Allows user to enter a walk mode.",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "vcmplugin build",
    "build": "vcmplugin build",
    "bundle": "vcmplugin bundle",
    "start": "vcmplugin serve",
    "preview": "vcmplugin preview",
    "buildStagingApp": "vcmplugin buildStagingApp",
    "lint:js": "eslint . --ext .vue,.js,.cjs,.mjs,.ts,.cts,.mts",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:prettier",
    "format": "prettier --write --list-different . && npm run lint:js -- --fix",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "type-check": "vue-tsc --noEmit",
    "ensure-types": "vcmplugin ensure-types"
  },
  "author": "Tobias Krauth <tkrauth@vc.systems>",
  "license": "MIT",
  "keywords": [
    "vcmap",
    "plugin"
  ],
  "files": [
    "src/",
    "dist/",
    "plugin-assets/",
    "LICENSE.md",
    "README.md",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": "dist/index.js",
    "./dist": "./dist/index.js"
  },
  "eslintIgnore": [
    "node_modules",
    "dist",
    "plugin-assets"
  ],
  "prettier": "@vcsuite/eslint-config/prettier.js",
  "peerDependencies": {
    "@vcmap-cesium/engine": "^11.0.2",
    "@vcmap/core": "^6.0.0-rc.12",
    "@vcmap/ui": "^6.0.0-rc.10",
    "vue": "~3.4.38",
    "vuetify": "^3.7.2"
  },
  "devDependencies": {
    "@vcmap/plugin-cli": "^4.0.0-rc.4",
    "@vcsuite/eslint-config": "^3.0.7",
    "@vitest/coverage-v8": "^2.0.3",
    "jest-canvas-mock": "^2.5.2",
    "jsdom": "^24.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "typescript": "^5.4.3",
    "vitest": "^2.0.3",
    "vue-tsc": "^2.0.7"
  },
  "mapVersion": "^6.0",
  "dependencies": {
    "@vcsuite/logger": "^1.0.1"
  }
}
